@model FileDetailsServiceModel

@{
    ViewBag.Title = "Delete Brs File";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

<form asp-action="Delete">
    <div class="row">
        <div class="col-md-8">
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" disabled="disabled" />
            </div>
            <div class="col-md-8">
                <div class="form-group">
                    <label asp-for="Code" class="control-label"></label>
                    <input asp-for="Code" class="form-control" disabled="disabled" />
                </div>

            <div class="form-group">
                <label asp-for="Dcng" class="control-label"></label>
                <input asp-for="Dcng" class="form-control" disabled="disabled" />
            </div>
            <div class="form-group">
                    <input asp-controller="WorkingOnFiles" asp-action="Delete" type="submit" value="Delete" class="btn btn-primary" />
            </div>
        </div>
    </div>
</form>