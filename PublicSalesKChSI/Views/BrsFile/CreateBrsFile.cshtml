@model List<BrsOnlyContent>
    
@{
    ViewBag.Title = "Съобщения от BCPEA с неупешна валидация/верификация";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

<div class="text-left row">
    @foreach (var message in Model)
    {
        <div class="card col-2" style="width: 20rem; ">
            <div class="card-body">
                <p class="card-title mt-1">@message.Title</p>
                <p class="card-title mt-1">@message.Date</p>
                <p class="card-title mt-1">@message.Price</p>
                <p class="card-title mt-1">@message.NameSI</p>
                <p class="card-title mt-1">@message.NumberSI</p>
                @foreach(string item in message.Other)
                {
                    <p class="card-title mt-1">@item</p>
                }
                <p class="card-title mt-1">@message.Text</p>

            </div>
        </div>
    }
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}




